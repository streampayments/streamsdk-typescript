{
  "name": "@streamsdk/typescript",
  "version": "1.0.1",
  "description": "Official Node.js/TypeScript SDK for Stream API - Complete payment processing with consumers, products, subscriptions, invoices, and payment links",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "keywords": [
    "stream",
    "payment",
    "sdk",
    "subscription",
    "invoice",
    "payment-gateway",
    "billing",
    "typescript",
    "saudi-arabia",
    "fintech"
  ],
  "author": "Stream <support@streampay.sa>",
  "contributors": [
    {
      "name": "Ibtisam",
      "email": "ibtisam@streampay.sa"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/streampayments/streamsdk-typescript"
  },
  "bugs": {
    "url": "https://github.com/streampayments/streamsdk-typescript/issues",
    "email": "support@streampay.sa"
  },
  "homepage": "https://github.com/streampayments/streamsdk-typescript#readme",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "gen": "openapi-typescript https://stream-app-service.streampay.sa/openapi.json -o src/generated/openapi.ts",
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "example": "node examples/basic.mjs"
  },
  "devDependencies": {
    "openapi-typescript": "^7.4.1",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
